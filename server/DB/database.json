{
    "usersInfo": {
        "id": "INT AUTO_INCREMENT PRIMARY KEY",
        "name": "VARCHAR(25)",
        "user_name": "VARCHAR(25)",
        "email": "VARCHAR(60)",
        "phone_number": "INT"
    },
    "photos": {
        "id": "INT AUTO_INCREMENT PRIMARY KEY",
        "album_id": "INT",
        "title": "VARCHAR(100)",
        "url": "BLOB",
        "FOREIGN KEY": "(album_id)" 
    },
    "albums": {
        "id": "INT AUTO_INCREMENT PRIMARY KEY",
        "user_id": "INT FOREIGN KEY",
        "title": "VARCHAR(25)"
    },
    "comments": {
        "id": "INT AUTO_INCREMENT PRIMARY KEY",
        "post_id": "INT FOREIGN KEY",
        "name": "VARCHAR(25)",
        "email": "VARCHAR(60)",
        "body": "BLOB"
    },
    "posts": {
        "id": "INT AUTO_INCREMENT PRIMARY KEY",
        "user_id": "INT FOREIGN KEY",
        "title": "VARCHAR(255)",
        "body": "BLOB"
    },
    "todos": {
        "id": "INT AUTO_INCREMENT PRIMARY KEY",
        "user_id": "INT FOREIGN KEY",
        "title": "VARCHAR(255)",
        "completed": "TINYINT(1)"
    },
    "password": {
        "id": "INT AUTO_INCREMENT PRIMARY KEY",
        "user_id": "INT FOREIGN KEY",
        "password": "VARCHAR(25)"
    } 
}